{
  "name": "falcon",
  "version": "0.1.0",
  "description": "a small and fast medicine viewer sdk by react",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "clean": "pnpm --filter=@falcon/* run clean && rimraf ./node_modules && pnpm i",
    "start": "pnpm  --filter=@falcon/* run  --parallel start",
    "build": "pnpm --filter=@falcon/* run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg-a06/ts_lib_tpl.git"
  },
  "keywords": [
    "ts",
    "react",
    "zustand",
    "react-query"
  ],
  "author": "xg-a06 <xg_jser@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xg-a06/ts_lib_tpl/issues"
  },
  "homepage": "https://github.com/xg-a06/ts_lib_tpl#readme",
  "devDependencies": {
    "@babel/runtime": "^7.18.6",
    "@changesets/cli": "^2.23.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@eslint/create-config": "^0.2.0",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "core-js": "^3.23.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "immer": "^9.0.15",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "zustand": "4.0.0-rc.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
